<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Theme.RetoTiendaVirtual" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/end_color</item>
        <item name="colorPrimaryVariant">@color/center_color</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>

        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

        <item name="android:fontFamily">@font/delius_swash_caps</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14dp</item>



    </style>

    <style name="Theme.RetoTiendaVirtual.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RetoTiendaVirtual.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RetoTiendaVirtual.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>